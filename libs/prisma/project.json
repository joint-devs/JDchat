{
  "name": "prisma",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "targets": {
    "prisma": {
      "command": "prisma",
      "options": {
        "cwd": "libs/prisma"
      }
    },
    "generate-types": {
      "command": "prisma generate",
      "options": {
        "cwd": "libs/prisma"
      }
    },
    "migrate": {
      "command": "prisma migrate dev --name typeChange",
      "options": {
        "cwd": "libs/prisma"
      }
    },
    "deploy": {
      "command": "prisma migrate deploy",
      "options": {
        "cwd": "libs/prisma"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": []
}
